package br.com.alcatellucent.csm.beans;

import java.util.Date;
import java.util.HashSet;
import java.util.Set;

import br.com.alcatellucent.csm.beans.common.CustomObject;

public class Scheduling extends CustomObject {

    private String dateStart;
    private String hour;
    private String minute;
    private String[] week;
    private String[] month;
    private String every;
    private String year;
    private String everyType;
    private Date dateInc;
    private String timeStart;
    private String daysOfWeek;
    private String months;
    private String contextId;
    private Set<ScheduledGroup> scheduledGroups;
    private String statusQuartz;
    private String cronExpression;
    private String trafficPolicyId;
    private String trafficPolicyName;
    private String cronSubscriber;

    /**
     * @return the trafficPolicyId
     */
    public String getTrafficPolicyId() {
	return trafficPolicyId;
    }

    /**
     * @param trafficPolicyId
     *                the trafficPolicyId to set
     */
    public void setTrafficPolicyId(String trafficPolicyId) {
	this.trafficPolicyId = trafficPolicyId;
    }

    public String getContextId() {
	return contextId;
    }

    public void setContextId(String contextId) {
	this.contextId = contextId;
    }

    public String getDaysOfWeek() {
	return daysOfWeek;
    }

    public void setDaysOfWeek(String daysOfWeek) {
	this.daysOfWeek = daysOfWeek;
    }

    public String getDateStart() {
	return dateStart;
    }

    public void setDateStart(String dateStart) {
	this.dateStart = dateStart;
    }

    public String getHour() {
	return hour;
    }

    public void setHour(String hour) {
	this.hour = hour;
    }

    public String getMinute() {
	return minute;
    }

    public void setMinute(String minute) {
	this.minute = minute;
    }

    public String[] getWeek() {
	return week;
    }

    public void setWeek(String[] week) {
	this.week = week;
    }

    public String[] getMonth() {
	return month;
    }

    public void setMonth(String[] month) {
	this.month = month;
    }

    public String getEvery() {
	return every;
    }

    public void setEvery(String every) {
	this.every = every;
    }

    public String getYear() {
	return year;
    }

    public void setYear(String year) {
	this.year = year;
    }

    public String getEveryType() {
	return everyType;
    }

    public void setEveryType(String everyType) {
	this.everyType = everyType;
    }

    /**
     * @return the dateInc
     */
    public Date getDateInc() {
	return dateInc;
    }

    /**
     * @param dateInc
     *                the dateInc to set
     */
    public void setDateInc(Date dateInc) {
	this.dateInc = dateInc;
    }

    public String getTimeStart() {
	return timeStart;
    }

    public void setTimeStart(String timeStart) {
	this.timeStart = timeStart;
    }

    public String getMonths() {
	return months;
    }

    public void setMonths(String months) {
	this.months = months;
    }

    /**
     * @return the scheduledGroups
     */
    public Set<ScheduledGroup> getScheduledGroups() {
	if (null == scheduledGroups)
	    scheduledGroups = new HashSet<ScheduledGroup>();
	return scheduledGroups;
    }

    /**
     * @param scheduledGroups
     *                the scheduledGroups to set
     */
    public void setScheduledGroups(Set<ScheduledGroup> scheduledGroups) {
	this.scheduledGroups = scheduledGroups;
    }

    public String getStatusQuartz() {
	return statusQuartz;
    }

    public void setStatusQuartz(String statusQuartz) {
	this.statusQuartz = statusQuartz;
    }

    public String getCronExpression() {
	return cronExpression;
    }

    public void setCronExpression(String cronExpression) {
	this.cronExpression = cronExpression;
    }

    /**
     * @return the trafficPolicyName
     */
    public String getTrafficPolicyName() {
	return trafficPolicyName;
    }

    /**
     * @param trafficPolicyName
     *                the trafficPolicyName to set
     */
    public void setTrafficPolicyName(String trafficPolicyName) {
	this.trafficPolicyName = trafficPolicyName;
    }

    public String getCronSubscriber() {
	return cronSubscriber;
    }

    public void setCronSubscriber(String cronSubscriber) {
	this.cronSubscriber = cronSubscriber;
    }

}
