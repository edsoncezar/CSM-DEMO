public enum CsmLogSeverity {
	
	LOW_SEVERITY(0), 
	MEDIUM_SEVERITY(1), 
	HIGH_SEVERITY(2),
	FATAL_SEVERITY(3);
	
	private final Integer value;
	
	CsmLogSeverity(Integer value) {

		this.value = value;
		
	}

	public Integer getValue() {
		return this.value;
	}
	
}
