<xmp>
	tree=new dhtmlXTreeObject("treeboxbox_tree1","100%","100%",0);
	tree.setImagePath("../imgs/csh_bluebooks/");
	tree.loadXML("tree_b.xml");
</xmp>
			</td>
		</tr>
		<tr>
