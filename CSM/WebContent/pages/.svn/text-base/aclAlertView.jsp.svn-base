<fieldset style="width:500; align:center" >
<html:form styleId="AclForm" method="post" action="">
	<table border="0" cellpadding="0" cellspacing="0" width="300" style="border : none;">		

		<tr>
			<td>Name</td>
			<td colspan="4"><b>${alertForm.acl.name}</b></td>
		</tr>
	
		
		<tr>
			<td>Description&nbsp;</td>
			<td colspan="3"><b>${alertForm.acl.description}</b></td>
			<td>&nbsp;&nbsp;&nbsp;</td>
		</tr>
		
		<tr>
			<td colspan="5">&nbsp;</td>
		</tr>
		
		<tr>
			<td></td>
			<td>Source</td>
			<td>Mask</td>
			<td>Destination</td>
			<td>Mask</td>
		</tr>
		
		<tr>
			<td>IP Address</td>
			<td><b>${alertForm.acl.sourceIp}</b></td>
			<td><b>${alertForm.acl.sourceIpMask}</b></td>
			<td><b>${alertForm.acl.destIp}</b></td>
			<td><b>${alertForm.acl.destIpMask}</b></td>
		</tr>
		
		<tr>
			<td>Port</td>
			<td><b>${alertForm.acl.sourcePort}</b></td>
			<td>&nbsp;</td>
			<td><b>${alertForm.acl.destPort}</b></td>
			<td>&nbsp;</td>
		</tr>
				
		<tr>
			<td colspan="5">&nbsp;</td>
		</tr>
	</table>
</html:form>
</fieldset>