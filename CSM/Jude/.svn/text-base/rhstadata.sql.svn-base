ALTER PROCEDURE POWER (
    NUMERO NUMERIC(9,2),
    EXPOENTE INTEGER)
RETURNS (
    RESULTADO NUMERIC(15,2))
AS
DECLARE VARIABLE CONTADOR INTEGER;
begin
  resultado = -1;
  if (:expoente < 0 ) then
    exception new_exception;

  contador = 0;
  resultado = 1;
  while (:contador < :EXPOENTE) do
  begin
    resultado = resultado * numero;
    contador = contador + 1;
  end
  when any do
  begin
    resultado = -1;
    exit;
  end
end
^

ALTER PROCEDURE SP_CHECK_COPIA (
    FUNID_ORIGEM CHAR(32),
    FUNID_DESTINO CHAR(32),
    EMPID_ORIGEM CHAR(32),
    EMPID_DESTINO CHAR(32),
    REGISTRO_DESTINO INTEGER)
AS
DECLARE VARIABLE CODIGO_RESCISAO CHAR(2);
DECLARE VARIABLE ESQUEMAOID_ORIGEM CHAR(32);
DECLARE VARIABLE ESQUEMAOID_DESTINO CHAR(32);
DECLARE VARIABLE SITUACAO CHAR(2);
begin

  SELECT CADFUN.situacao FROM CADFUN
  WHERE
    CADFUN.funid = :funid_origem AND
    CADFUN.EMPID = :empid_origem
  INTO :situacao;


  if (:situacao <> '1') then
      exception excep_geral 'Funcionário não ativo';

  SELECT
    RESCISAO.CODIGO
  FROM
    rescisao
  WHERE
    RESCISAO.funid = :funid_origem AND
    RESCISAO.EMPID = :empid_origem
  INTO :CODIGO_RESCISAO;

  if (:codigo_rescisao <> '00') then
      exception excep_geral 'Funcionário não ativo';

  SELECT EMPRESA.uosid FROM EMPRESA
  WHERE EMPRESA.empid = :empid_origem
  INTO :esquemaoid_origem;


  SELECT EMPRESA.uosid FROM EMPRESA
  WHERE EMPRESA.empid = :empid_destino
  INTO :esquemaoid_destino;


  if (:esquemaoid_origem <>  :esquemaoid_destino) then
      exception excep_geral 'Empresa origem e destino com esquemas organizacionais diferentes';


end
^

ALTER PROCEDURE SP_CHECK_READMISSAO (
    FUNID_ORIGEM CHAR(32),
    FUNID_DESTINO CHAR(32),
    EMPID_ORIGEM CHAR(32),
    EMPID_DESTINO CHAR(32),
    REGISTRO_DESTINO INTEGER)
AS
DECLARE VARIABLE CODIGO_RESCISAO CHAR(2);
DECLARE VARIABLE EXISTE_REGISTRO INTEGER;
DECLARE VARIABLE FLAG_READMISSAO SMALLINT;
DECLARE VARIABLE SITUACAO CHAR(2);
begin

  SELECT CADFUN.situacao FROM CADFUN
  WHERE
    CADFUN.funid = :funid_origem AND
    CADFUN.EMPID = :empid_origem
  INTO :situacao;


  if (:situacao <> '9') then
      exception excep_geral 'Funcionário NÃO inativo';




  /*
    Pego codigo de rescisao do funcionario origem
  */
  SELECT
    RESCISAO.CODIGO, RESCISAO.fl_readmissao
  FROM
    rescisao
  WHERE
    RESCISAO.funid = :funid_origem AND
    RESCISAO.EMPID = :empid_origem
  INTO :CODIGO_RESCISAO, :flag_readmissao;

  if (:codigo_rescisao = '00') then
      exception excep_geral 'Funcionário ativo.';

  if ( (:codigo_rescisao = '30') or (:codigo_rescisao = '31')  ) then
      exception excep_geral 'Funcionário transferido.';

  if (:flag_readmissao <> 1) then
      exception excep_geral 'Funcionário não pode ser readmitido (Flag na rescisao)';

    select COUNT(cadfun.registro) from cadfun where
    cadfun.empid = :empid_destino and
    cadfun.registro = :registro_destino
    INTO :existe_registro;

  if (:existe_registro <> 0) then
      exception excep_geral 'Registro destino já existente';

end
^

