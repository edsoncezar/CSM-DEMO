    WCODIGO,
    WRESPONSAVEL,
    WDESCRICAO,
    WNOMENIVEL,
    WGRAU,
    WPOSINI,
    WPOSFIM)
AS
SELECT
  UNIORG.UOSID WUOSID,
  UNIORG.UORID WUORID,
  SUBSTR(UNIORG.UORID,UNIORGNIVEL.POS_INI,UNIORGNIVEL.POS_FIM) WCODIGO,
  UNIORG.RESPONSAVEL WRESPONSAVEL,
  UNIORG.DESCRICAO WDESCRICAO,
  UNIORGNIVEL.NOME WNOMENIVEL,
  UNIORG.GRAU WGRAU,
  UNIORGNIVEL.POS_INI WPOSINI,
  UNIORGNIVEL.POS_FIM WPOSFIM
FROM
  UNIORGNIVEL
INNER JOIN UNIORG ON (UNIORGNIVEL.UOSID = UNIORG.UOSID) AND (UNIORGNIVEL.NIVEL = UNIORG.GRAU)
;



