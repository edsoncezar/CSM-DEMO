ALTER PROCEDURE USP_INSERELANCTO (
    FUNC_REGISTRO INTEGER,
    CODIGO_EMPRESA VARCHAR(6),
    MES INTEGER,
    ANO INTEGER,
    TRIBUTAVEL DOUBLE PRECISION,
    IRRF DOUBLE PRECISION,
    PREVIDENCIA DOUBLE PRECISION)
RETURNS (
    SEILA VARCHAR(38))
AS
DECLARE VARIABLE FUNID CHAR(38);
DECLARE VARIABLE EMPID CHAR(38);
DECLARE VARIABLE EVENTOID CHAR(38);
DECLARE VARIABLE LANCTO INTEGER;
begin

  SELECT EMPID AS SEILA FROM EMPRESA WHERE
  EMPRESA.CODIGO = :CODIGO_EMPRESA INTO :EMPID;

  SELECT
    FUNID
  FROM
   CADFUN WHERE (CADFUN.REGISTRO = :FUNC_REGISTRO AND CADFUN.EMPID = :EMPID)
  INTO
    :FUNID;




  SELECT COUNT(*) FROM GPLANCTO
  WHERE EMPID=:EMPID AND FUNID = :FUNID AND MES = :MES AND ANO = :ANO INTO :LANCTO;


  if (:LANCTO = 0) then
  BEGIN
    INSERT INTO GPLANCTO (FUNID, EMPID, MES, ANO) VALUES (:FUNID, :EMPID,  :MES, :ANO);
    SELECT COUNT(*) FROM GPLANCTO
    WHERE EMPID=:EMPID AND FUNID = :FUNID AND MES = :MES AND ANO = :ANO INTO :SEILA;
    SUSPEND;
  END



  SELECT EVEID FROM EVENTOS
    WHERE
  EVENTOS.GRTID = '001' AND EVENTOS.CODIGO = 801 INTO :EVENTOID;


  DELETE FROM GPLANCTODET WHERE
    EVEID = :EVENTOID AND
    FUNID = :FUNID AND
    MES = :MES AND
    ANO = :ANO;

  INSERT INTO GPLANCTODET
    (FUNID, MES, ANO,
    EVEID, TIPO_REG, SEQUENCIA, REFERENCIA, VALOR,
    FL_TIP_LANCTO, CODIGO_EVENTO) VALUES (:FUNID, :MES, :ANO, :EVENTOID,
    'FL', 1, 0, :TRIBUTAVEL, 'L', 801);


  SELECT EVEID FROM EVENTOS
    WHERE
  EVENTOS.GRTID = '001' AND EVENTOS.CODIGO = 401 INTO :EVENTOID;

  DELETE FROM GPLANCTODET WHERE
    EVEID = :EVENTOID AND
    FUNID = :FUNID AND
    MES = :MES AND
    ANO = :ANO;

  INSERT INTO GPLANCTODET
    (FUNID, MES, ANO,
    EVEID, TIPO_REG, SEQUENCIA, REFERENCIA, VALOR,
    FL_TIP_LANCTO, CODIGO_EVENTO) VALUES (:FUNID, :MES, :ANO, :EVENTOID,
    'FL', 1, 0, :IRRF, 'L', 401);


  SELECT EVEID FROM EVENTOS
    WHERE
  EVENTOS.GRTID = '001' AND EVENTOS.CODIGO = 410 INTO :EVENTOID;

  DELETE FROM GPLANCTODET WHERE
    EVEID = :EVENTOID AND
    FUNID = :FUNID AND
    MES = :MES AND
    ANO = :ANO;

  INSERT INTO GPLANCTODET
    (FUNID, MES, ANO,
    EVEID, TIPO_REG, SEQUENCIA, REFERENCIA, VALOR,
    FL_TIP_LANCTO, CODIGO_EVENTO) VALUES (:FUNID, :MES, :ANO, :EVENTOID,
    'FL', 1, 0, :PREVIDENCIA, 'L', 410);

end
^

