ALTER PROCEDURE CONSULTAEVENTOID (
    CODIGO INTEGER,
    USUID VARCHAR(38),
    MULTIPROC INTEGER)
RETURNS (
    EVEID VARCHAR(38))
AS
BEGIN
    IF (:MULTIPROC = 1)
    THEN
      FOR
        select distinct EVENTOS.EVEID from multiproc
        inner join EMPRESATABELAS on (MULTIPROC.EMPID = EMPRESATABELAS.EMPID and EMPRESATABELAS.TABELA = 'GIP-EVENTO')
        inner join EVENTOS on (EVENTOS.GRTID = EMPRESATABELAS.GRTID and EVENTOS.CODIGO = :CODIGO)
        INTO :EVEID
      DO
        SUSPEND;
    ELSE
      FOR
        select EVENTOS.EVEID from USUARIO
        inner join EMPRESATABELAS on (USUARIO.EMPDEFAULT = EMPRESATABELAS.EMPID and EMPRESATABELAS.TABELA = 'GIP-EVENTO')
        inner join EVENTOS on (EVENTOS.GRTID = EMPRESATABELAS.GRTID and EVENTOS.CODIGO = :CODIGO)
        where USUARIO.USUID = :USUID
        INTO :EVEID
      DO
        SUSPEND;
  END
^

ALTER PROCEDURE COPIA_BENEFICIOS (
    EMPID_ORIGEM VARCHAR(38),
    FUNID_ORIGEM VARCHAR(38),
    EMPID_DESTINO CHAR(32),
    FUNID_DESTINO CHAR(32))
AS
DECLARE VARIABLE FUNID VARCHAR(38);
DECLARE VARIABLE MES INTEGER;
DECLARE VARIABLE ANO INTEGER;
DECLARE VARIABLE PARCELA INTEGER;
DECLARE VARIABLE VALEID VARCHAR(38);
DECLARE VARIABLE MES_L INTEGER;
DECLARE VARIABLE ANO_L INTEGER;
DECLARE VARIABLE PARCELA_L INTEGER;
DECLARE VARIABLE VALEID_L VARCHAR(38);
