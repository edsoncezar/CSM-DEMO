/* View: VW_OBJETOS_CONTEXTO */
CREATE VIEW VW_OBJETOS_CONTEXTO(
    AID,
    ACODIGO,
    ANOME,
    ACONTEXTO,
    ATIPO)
AS
select
    empresa.empid aid,
    cast(empresa.codigo as varchar(70)) acodigo,
    empresa.nome anome,
    empresa.contexto acontexto,
    cast('E' as varchar(1)) atipo
from
    empresa
union
select
    usuario.usuid aid,
    usuario.username acodigo,
    usuario.nome anome,
    usuario.contexto acontexto,
    cast('U' as varchar(1)) atipo
from
    usuario
union
select
    perfil.prfid aid,
    cast(perfil.prfid as varchar(70)) acodigo,
    perfil.nome anome,
    perfil.contexto acontexto,
    cast('P' as varchar(1)) atipo
from
    perfil
union
select
    contexto.contextid aid,
    cast(contexto.contextid as varchar(70)) acodigo,
    cast(contexto.nomecontexto as varchar(70)) anome,
    contexto.contextid acontexto,
    cast('C' as varchar(1)) atipo
from
    contexto
;



