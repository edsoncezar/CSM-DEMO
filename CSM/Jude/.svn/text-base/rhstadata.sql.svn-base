DECLARE VARIABLE DAT_FERIAS_DE TIMESTAMP;
DECLARE VARIABLE DAT_FERIAS_ATE TIMESTAMP;
DECLARE VARIABLE DAT_AFAST_DE TIMESTAMP;
DECLARE VARIABLE DAT_AFAST_ATE TIMESTAMP;
DECLARE VARIABLE DIAS_USO INTEGER;
DECLARE VARIABLE PARID VARCHAR(38);
DECLARE VARIABLE UNEID VARCHAR(38);
DECLARE VARIABLE VALOR_EMPRESA DOUBLE PRECISION;
DECLARE VARIABLE VALOR_FUNCIONARIO DOUBLE PRECISION;
DECLARE VARIABLE VALOR_TOTAL DOUBLE PRECISION;
DECLARE VARIABLE EVEIDDEBITO VARCHAR(38);
