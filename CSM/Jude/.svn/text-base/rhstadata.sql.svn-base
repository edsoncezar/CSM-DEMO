DECLARE VARIABLE SEQUENCIA INTEGER;
DECLARE VARIABLE FAIXA SMALLINT;
DECLARE VARIABLE BENID VARCHAR(38);
DECLARE VARIABLE NOME_BENEFICIO VARCHAR(50);
DECLARE VARIABLE CODIGO_BENEFICIO INTEGER;
DECLARE VARIABLE BLOCOS INTEGER;
DECLARE VARIABLE SOLTOS INTEGER;
DECLARE VARIABLE QUANTIDADE INTEGER;
DECLARE VARIABLE VALOR_UNITARIO DOUBLE PRECISION;
DECLARE VARIABLE EVEID VARCHAR(38);
DECLARE VARIABLE VALOR FLOAT;
BEGIN

  if (
     (:funid_destino is null) or (:funid_destino = '') or
     (:funid_origem is null)  or (:funid_origem = '') or
     (:empid_origem is null)  or (:empid_origem = '') or
     (:empid_destino is null) or (:empid_destino = '')  ) then

      exception excep_geral 'Argumentos inválidos'  ;


  FOR SELECT FUNID,
             MES,
             ANO,
             PARCELA,
             VALEID,
             FL_ESTOQUE,
             DAT_FERIAS_DE,
             DAT_FERIAS_ATE,
             DAT_AFAST_DE,
             DAT_AFAST_ATE,
             DIAS_USO,
             PARID,
             UNEID,
             VALOR_EMPRESA,
             VALOR_FUNCIONARIO,
             VALOR_TOTAL,
             EVEIDDEBITO,
             EVEIDCREDITO,
             EVEIDEMPRESA,
             CAL_PERIODO_DE,
             CAL_PERIODO_ATE,
             DIAS_PERIODO,
             DIAS_UTEIS,
             DIAS_CAL,
             CALID,
             TIPO,
             SEQUENCIA,
             FAIXA
  FROM GPCADFUNVB
  WHERE
    FUNID = :FUNID_ORIGEM
  INTO :FUNID,
       :MES_L,
       :ANO_L,
       :PARCELA_L,
       :VALEID_L,
       :FL_ESTOQUE,
       :DAT_FERIAS_DE,
       :DAT_FERIAS_ATE,
       :DAT_AFAST_DE,
       :DAT_AFAST_ATE,
       :DIAS_USO,
       :PARID,
       :UNEID,
       :VALOR_EMPRESA,
       :VALOR_FUNCIONARIO,
       :VALOR_TOTAL,
       :EVEIDDEBITO,
       :EVEIDCREDITO,
       :EVEIDEMPRESA,
       :CAL_PERIODO_DE,
       :CAL_PERIODO_ATE,
       :DIAS_PERIODO,
       :DIAS_UTEIS,
       :DIAS_CAL,
       :CALID,
       :TIPO,
       :SEQUENCIA,
       :FAIXA
  DO
  BEGIN

  INSERT INTO GPCADFUNVB (
    FUNID,
    MES,
    ANO,
    PARCELA,
    VALEID,
    FL_ESTOQUE,
    DAT_FERIAS_DE,
    DAT_FERIAS_ATE,
    DAT_AFAST_DE,
    DAT_AFAST_ATE,
    DIAS_USO,
    PARID,
    UNEID,
    VALOR_EMPRESA,
    VALOR_FUNCIONARIO,
    VALOR_TOTAL,
    EVEIDDEBITO,
    EVEIDCREDITO,
    EVEIDEMPRESA,
    CAL_PERIODO_DE,
    CAL_PERIODO_ATE,
    DIAS_PERIODO,
    DIAS_UTEIS,
    DIAS_CAL,
    CALID,
    TIPO,
    SEQUENCIA,
    FAIXA)
  VALUES (
    :FUNID_DESTINO,
    :MES_L,
    :ANO_L,
    :parcela_L,
    :valeid_L,
    :FL_ESTOQUE,
    :DAT_FERIAS_DE,
    :DAT_FERIAS_ATE,
    :DAT_AFAST_DE,
    :DAT_AFAST_ATE,
    :DIAS_USO,
    :PARID,
    :UNEID,
    :VALOR_EMPRESA,
    :VALOR_FUNCIONARIO,
    :VALOR_TOTAL,
    :EVEIDDEBITO,
    :EVEIDCREDITO,
    :EVEIDEMPRESA,
    :CAL_PERIODO_DE,
    :CAL_PERIODO_ATE,
    :DIAS_PERIODO,
    :DIAS_UTEIS,
    :DIAS_CAL,
    :CALID,
    :TIPO,
    :SEQUENCIA,
    :FAIXA);


    FOR SELECT FUNID,
             MES,
             ANO,
             PARCELA,
             VALEID,
             BENID,
             NOME_BENEFICIO,
             CODIGO_BENEFICIO,
             BLOCOS,
             SOLTOS,
             QUANTIDADE,
             VALOR_UNITARIO
  FROM GPCADFUNVBDET
  WHERE
    FUNID = :funid_origem AND
    MES = :MES_L AND
    ANO = :ano_l AND
    PARCELA = :parcela_l AND
    VALEID = :valeid_l
  INTO :FUNID,
       :MES,
       :ANO,
       :PARCELA,
       :VALEID,
       :BENID,
       :NOME_BENEFICIO,
       :CODIGO_BENEFICIO,
       :BLOCOS,
       :SOLTOS,
       :QUANTIDADE,
       :VALOR_UNITARIO
  DO
    BEGIN
  INSERT INTO GPCADFUNVBDET (
    FUNID,
    MES,
    ANO,
    PARCELA,
    VALEID,
    BENID,
    NOME_BENEFICIO,
    CODIGO_BENEFICIO,
    BLOCOS,
    SOLTOS,
    QUANTIDADE,
    VALOR_UNITARIO)
  VALUES (
    :FUNID_DESTINO,
    :MES,
    :ANO,
    :PARCELA,
    :VALEID,
    :BENID,
    :NOME_BENEFICIO,
    :CODIGO_BENEFICIO,
    :BLOCOS,
    :SOLTOS,
    :QUANTIDADE,
    :VALOR_UNITARIO);
    END




    /* Vale Det */

  FOR SELECT FUNID,
             ANO,
             MES,
             PARCELA,
             VALEID,
             EVEID,
             VALOR
  FROM VALEDET
  WHERE
    FUNID = :funid_origem AND
    MES = :MES_L AND
    ANO = :ano_l AND
    PARCELA = :parcela_l AND
    VALEID = :valeid_l
  INTO :FUNID,
       :ANO,
       :MES,
       :PARCELA,
       :VALEID,
       :EVEID,
       :VALOR
  DO
    BEGIN
  INSERT INTO VALEDET (
    FUNID,
    ANO,
    MES,
    PARCELA,
    VALEID,
    EVEID,
    VALOR)
  VALUES (
    :FUNID_DESTINO,
    :ANO,
    :MES,
    :PARCELA,
    :VALEID,
    :EVEID,
    :VALOR);

    END



  END


END
^

ALTER PROCEDURE COPIA_CADFUN (
    EMPID_ORIGEM VARCHAR(38),
    FUNID_ORIGEM VARCHAR(38),
    EMPID_DESTINO CHAR(32),
    FUNID_DESTINO CHAR(32),
    REGISTRO_DESTINO INTEGER)
AS
DECLARE VARIABLE REGISTRO INTEGER;
DECLARE VARIABLE DRT INTEGER;
DECLARE VARIABLE SITUACAO VARCHAR(2);
DECLARE VARIABLE UNIORG VARCHAR(35);
DECLARE VARIABLE CCUSTOGERID VARCHAR(38);
DECLARE VARIABLE CCUSTOCONID VARCHAR(38);
DECLARE VARIABLE APELIDO VARCHAR(15);
DECLARE VARIABLE DAT_ADMISSAO TIMESTAMP;
DECLARE VARIABLE ESTABILIDADE_DATA_INICIO TIMESTAMP;
DECLARE VARIABLE ESTABILIDADE_DATA_FIM TIMESTAMP;
DECLARE VARIABLE ESTABILIDADE_MOTIVO VARCHAR(60);
DECLARE VARIABLE EXPERIENCIA_DIAS SMALLINT;
DECLARE VARIABLE EXPERIENCIA_DIAS_PRORROGACAO SMALLINT;
DECLARE VARIABLE LIVRE_TXT1 VARCHAR(15);
DECLARE VARIABLE LIVRE1 VARCHAR(30);
DECLARE VARIABLE LIVRE_TXT2 VARCHAR(15);
DECLARE VARIABLE LIVRE2 VARCHAR(30);
DECLARE VARIABLE LIVRE_TXT3 VARCHAR(15);
DECLARE VARIABLE LIVRE3 VARCHAR(30);
DECLARE VARIABLE LOCACAO VARCHAR(30);
DECLARE VARIABLE FOTO BLOB SUB_TYPE 0 SEGMENT SIZE 80;
DECLARE VARIABLE EMAIL VARCHAR(50);
DECLARE VARIABLE ID_FUNCIONARIO_GIP INTEGER;
begin

  if (
     (:funid_destino is null) or (:funid_destino = '') or
     (:funid_origem is null)  or (:funid_origem = '') or
     (:empid_origem is null)  or (:empid_origem = '') or
     (:empid_destino is null) or (:empid_destino = '')  ) then

      exception excep_geral 'Argumentos inválidos'  ;



  if (exists(select funid, empid from cadfun
             where     funid=:funid_destino
                   and empid=:empid_destino)) then
     exception excep_geral 'Funcionario já existe na empresa destino (cadfun)';


  SELECT
       REGISTRO,
       DRT,
       SITUACAO,
       UNIORG,
       CCUSTOGERID,
       CCUSTOCONID,
       APELIDO,
       EMAIL,
       DAT_ADMISSAO,
       ESTABILIDADE_DATA_INICIO,
       ESTABILIDADE_DATA_FIM,
       ESTABILIDADE_MOTIVO,
       EXPERIENCIA_DIAS,
       EXPERIENCIA_DIAS_PRORROGACAO,
       LIVRE_TXT1,
       LIVRE1,
      LIVRE_TXT2,
       LIVRE2,
       LIVRE_TXT3,
       LIVRE3,
       LOCACAO,
       FOTO,
       ID_FUNCIONARIO_GIP
  FROM CADFUN
  WHERE
       FUNID = :funid_origem
   AND EMPID = :empid_origem
  INTO
       :REGISTRO,
       :DRT,
       :SITUACAO,
       :UNIORG,
       :CCUSTOGERID,
       :CCUSTOCONID,
       :APELIDO,
       :EMAIL,
       :DAT_ADMISSAO,
       :ESTABILIDADE_DATA_INICIO,
       :ESTABILIDADE_DATA_FIM,
       :ESTABILIDADE_MOTIVO,
       :EXPERIENCIA_DIAS,
       :EXPERIENCIA_DIAS_PRORROGACAO,
       :LIVRE_TXT1,
       :LIVRE1,
       :LIVRE_TXT2,
       :LIVRE2,
       :LIVRE_TXT3,
       :LIVRE3,
       :LOCACAO,
       :FOTO,
       :ID_FUNCIONARIO_GIP;

  if (exists(
          select registro from cadfun
          where cadfun.empid = :empid_destino
             AND REGISTRO = :registro_destino
          )) then
        exception  excep_geral 'O Registro já existe na empresa destino';

  INSERT INTO CADFUN (
    EMPID,
    FUNID,
    REGISTRO,
    DRT,
    SITUACAO,
    UNIORG,
    CCUSTOGERID,
    CCUSTOCONID,
    APELIDO,
    EMAIL,
    DAT_ADMISSAO,
    ESTABILIDADE_DATA_INICIO,
    ESTABILIDADE_DATA_FIM,
    ESTABILIDADE_MOTIVO,
    EXPERIENCIA_DIAS,
    EXPERIENCIA_DIAS_PRORROGACAO,
    LIVRE_TXT1,
    LIVRE1,
    LIVRE_TXT2,
    LIVRE2,
    LIVRE_TXT3,
    LIVRE3,
    LOCACAO,
    FOTO,
    ID_FUNCIONARIO_GIP)
  VALUES (
    :empid_destino,
    :funid_destino,
    :registro_destino,
    :DRT,
    :SITUACAO,
    :UNIORG,
    :CCUSTOGERID,
    :CCUSTOCONID,
    :APELIDO,
    :EMAIL,
    :DAT_ADMISSAO,
    :ESTABILIDADE_DATA_INICIO,
    :ESTABILIDADE_DATA_FIM,
    :ESTABILIDADE_MOTIVO,
    :EXPERIENCIA_DIAS,
    :EXPERIENCIA_DIAS_PRORROGACAO,
    :LIVRE_TXT1,
    :LIVRE1,
    :LIVRE_TXT2,
    :LIVRE2,
    :LIVRE_TXT3,
    :LIVRE3,
    :LOCACAO,
    :FOTO,
    :ID_FUNCIONARIO_GIP);



END
^

