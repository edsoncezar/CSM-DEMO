ALTER PROCEDURE SPC_PROCADM (
    EMPID VARCHAR(38),
    GRTID CHAR(38),
    REGISTRO VARCHAR(10))
RETURNS (
    FUNID CHAR(38),
    SEQUENCIA INTEGER,
    DESCR VARCHAR(60),
    DIA TIMESTAMP,
    REGI VARCHAR(10))
AS
DECLARE VARIABLE QUANTIDADE INTEGER;
DECLARE VARIABLE CONTADOR INTEGER;
DECLARE VARIABLE GRUPOOID CHAR(38);
DECLARE VARIABLE FUNCIONARIOOID VARCHAR(38);
DECLARE VARIABLE DATA TIMESTAMP;
DECLARE VARIABLE OK INTEGER;
DECLARE VARIABLE DESCRI VARCHAR(60);
BEGIN
  /* Procedure Text */
  GRUPOOID = :GRTID;

  SELECT COUNT(*) FROM PROCADMITEM WHERE
    PROCADMITEM.GRTID = :GRUPOOID  INTO :QUANTIDADE;


  FOR
    SELECT DISTINCT FUNID FROM PROCADM INNER JOIN PROCADMITEM ON
     PROCADM.ADMID = PROCADMITEM.ADMID
    WHERE PROCADMITEM.GRTID = :GRUPOOID
      AND CAST(PROCADM.REGISTRO AS VARCHAR(6)) LIKE :REGISTRO
      AND EMPID=:EMPID


     INTO :FUNCIONARIOOID DO
  BEGIN

    FUNID = :FUNCIONARIOOID;
    CONTADOR = 1;
    OK = 0;
    FOR
      SELECT
        PROCADM.ENCERRADO,
        PROCADMITEM.SEQUENCIA,
        PROCADMITEM.DESCRICAO,
        PROCADM.REGISTRO
      FROM
        PROCADM INNER JOIN PROCADMITEM ON
         PROCADM.ADMID = PROCADMITEM.ADMID
      WHERE PROCADMITEM.GRTID = :GRUPOOID AND
            PROCADM.FUNID = :FUNCIONARIOOID
      INTO :DIA, :SEQUENCIA, :DESCR, :REGI DO
      BEGIN
          IF ((:OK =0 ) AND (:DIA IS NULL)) THEN
          BEGIN
            CONTADOR = (:SEQUENCIA - 1);
            OK = 1;
            SEQUENCIA = :CONTADOR;
            DIA = :DATA;
            DESCR = :DESCRI;
            SUSPEND;
          END
          DATA = :DIA;
          DESCRI = :DESCR;
          REGI = REGISTRO;
       END
  END


END
^

ALTER PROCEDURE UPDATEUNIORG
