ALTER PROCEDURE USP_TESTE (
    FUNCIONARIOOID CHAR(32),
    ANO_INPUT INTEGER,
    EMPRESAOID CHAR(32))
RETURNS (
    DESCRICAO VARCHAR(60),
    CODIGO INTEGER,
    VALOR DOUBLE PRECISION)
AS
DECLARE VARIABLE EVENTOOID VARCHAR(32);
begin
  FOR
    SELECT EVEID, CODIGO, DESCRICAO
      FROM EVENTOS
      WHERE FL_RAIS = 1
      ORDER BY CODIGO
      INTO :eventooid,:codigo,:descricao DO
    BEGIN
      VALOR = 0;
      SELECT SUM(gplanctodet.VALOR)
          FROM GPLANCTODET INNER JOIN
            GPLANCTO ON
              GPLANCTO.funid = GPLANCTODET.funid AND
              GPLANCTO.MES = GPLANCTODET.mes AND
              GPLANCTO.ano = GPLANCTODET.ano
          WHERE gplanctodet.EVEID=:eventooid AND
                gplanctodet.ANO=:ano_input AND
                gplanctodet.FUNID=:funcionariooid and
                gplancto.empid=:empresaoid
                and GPLANCTODET.tipo_reg = 'RA'
          GROUP BY gplanctodet.ANO
          INTO :valor  ;
      SUSPEND;
    END
end
^

ALTER PROCEDURE USP_TOTAL_DIRF (
    FUNCIONARIOOID CHAR(32),
    ANO_INPUT INTEGER,
    EMPRESAOID CHAR(32))
RETURNS (
    DESCRICAO VARCHAR(60),
    CODIGO INTEGER,
    VALOR DOUBLE PRECISION)
AS
DECLARE VARIABLE EVENTOOID VARCHAR(32);
begin
  FOR
    SELECT EVEID, CODIGO, DESCRICAO
      FROM EVENTOS
      WHERE FL_DIRF=1
      ORDER BY CODIGO
      INTO :eventooid,:codigo,:descricao DO
    BEGIN
      VALOR = 0;
      SELECT SUM(gplanctodet.VALOR)
          FROM GPLANCTODET INNER JOIN
            GPLANCTO ON
              GPLANCTO.funid = GPLANCTODET.funid AND
              GPLANCTO.MES = GPLANCTODET.mes AND
              GPLANCTO.ano = GPLANCTODET.ano
          WHERE gplanctodet.EVEID=:eventooid AND
                gplanctodet.ANO=:ano_input AND
                gplanctodet.FUNID=:funcionariooid and
                gplancto.empid=:empresaoid
                and GPLANCTODET.tipo_reg = 'DI'
          GROUP BY gplanctodet.ANO
          INTO :valor  ;
      SUSPEND;
    END
end
^

