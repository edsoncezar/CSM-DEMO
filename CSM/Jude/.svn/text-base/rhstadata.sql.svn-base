ALTER PROCEDURE SPC_EMP (
    USUID CHAR(38))
RETURNS (
    EMPID CHAR(38))
AS
DECLARE VARIABLE PERFILOID CHAR(32);
DECLARE VARIABLE CONTEXTID CHAR(32);
BEGIN

    SELECT
       USUARIO.CONTEXTO, USUARIO.PRFID
      FROM
        USUARIO
    WHERE                                                  
      USUARIO.USUID = :USUID
    INTO :CONTEXTID, :perfiloid;


  FOR
    SELECT
      EMPRESA.EMPID
    FROM
      EMPRESA
    WHERE
    (
      EMPRESA.CONTEXTO = :CONTEXTID OR
      EMPRESA.CONTEXTO IN ( SELECT SPC_CONTEXTOS.CONTEXTID FROM SPC_CONTEXTOS(:CONTEXTID) )
    ) AND not EMPRESA.empid IN
     ( SELECT acessoperfilempresa.empid FROM acessoperfilempresa where acessoperfilempresa.prfid = :perfiloid )
    INTO
      :EMPID
  DO
  SUSPEND;
END
^

ALTER PROCEDURE SPC_EMPRESACONTEXTO (
    LITERAL VARCHAR(30),
    USUID VARCHAR(38))
RETURNS (
    EMPID VARCHAR(38),
    CODIGO VARCHAR(30),
    NOME VARCHAR(60))
AS
DECLARE VARIABLE CONTEXTOID VARCHAR(38);
begin

    SELECT
       CONTEXTO.CONTEXTID
      FROM CONTEXTO INNER JOIN
        PERFIL ON PERFIL.CONTEXTO = CONTEXTO.CONTEXTID INNER JOIN
        USUARIO ON USUARIO.PRFID = PERFIL.PRFID
    WHERE                                                  
      USUARIO.USUID = :USUID INTO    :CONTEXTOID;

  FOR
    SELECT
      EMPID, CODIGO, NOME FROM  EMPRESA
    WHERE
    (
      EMPRESA.CONTEXTO = :CONTEXTOID
      or (  empresa.CONTEXTO in (
                 select CONTEXTO.CONTEXTID from CONTEXTO where parent = :CONTEXTOID)
         )
    ) AND  ( UPPER(NOME) LIKE UPPER(:LITERAL) or (CODIGO LIKE :LITERAL)  )


      INTO :EMPID, :CODIGO, :NOME DO
  BEGIN
    suspend;
  END
end
^

