ALTER PROCEDURE DEPARAGPLANCTO (
    MESDEST SMALLINT,
    ANODEST SMALLINT,
    FUNID VARCHAR(38),
    ANOORIGEM SMALLINT,
    MESORIGEM SMALLINT)
AS
DECLARE VARIABLE FL_PROCESSO CHAR(1);
DECLARE VARIABLE DAT_PAGTO TIMESTAMP;
DECLARE VARIABLE CARID VARCHAR(38);
DECLARE VARIABLE UNIORG VARCHAR(35);
DECLARE VARIABLE FL_FECHTO CHAR(1);
DECLARE VARIABLE CCUSTO_G VARCHAR(38);
DECLARE VARIABLE CCUSTO_C VARCHAR(38);
DECLARE VARIABLE EMPID VARCHAR(38);
begin
      select
        GPLANCTO.empid, GPLANCTO.ccusto_c, GPLANCTO.ccusto_g,
        GPLANCTO.fl_fechto, GPLANCTO.uniorg, GPLANCTO.carid,
        GPLANCTO.dat_pagto, GPLANCTO.fl_processo
      from
        gplancto
      where
        gplancto.funid = :funid and
        GPLANCTO.mes = :mesorigem and
        GPLANCTO.ano = :anoorigem
      into
        :EMPID, :ccusto_c, :ccusto_g,
        :fl_fechto, :uniorg, :carid,
        :dat_pagto, :fl_processo;


        insert into GPLANCTO
          (GPLANCTO.FUNID, GPLANCTO.ANO,
           GPLANCTO.MES, GPLANCTO.EMPID,
           GPLANCTO.CCUSTO_C, GPLANCTO.CCUSTO_G,
           GPLANCTO.FL_FECHTO, GPLANCTO.uniorg,
           GPLANCTO.CARID, GPLANCTO.DAT_PAGTO,
           GPLANCTO.FL_PROCESSO )
        values (
          :funid, :anodest,
          :mesdest, :empid,
          :ccusto_c, :ccusto_g,
          :fl_fechto, :uniorg,
          :carid, :dat_pagto,
          :fl_processo
        );
end
^

