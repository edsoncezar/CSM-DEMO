<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class name="br.com.alcatellucent.csm.beans.GroupTrafficPolicy"  table="group_traffic_policy"  lazy="false">

        <id	name="id" column="id"  type="java.lang.String" >
              <generator class="guid"></generator> 
        </id>

        
	
		            
 		<property  name="name"
		            type="java.lang.String"
		            update="true"
		            insert="true"
		            column="name" />		            
		            
		            

        <property   name="idTrafficPolice"
		            type="java.lang.String"
		            update="true"
		            insert="true"
		            column="id_traffic_police"/>
		            
		            

		

    </class>
</hibernate-mapping>
