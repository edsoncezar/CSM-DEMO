package teste;

import java.util.regex.Pattern;

public class TesteRegex {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		
		String string = "aaa";
		
		@SuppressWarnings("unused")
		Boolean a=Pattern.compile("\\w+[^0-9]").matcher(string).matches();
		Boolean b=Pattern.compile("\\d+").matcher(string).matches();
		Boolean c=Pattern.compile("\\w+\\d+").matcher(string).matches();
		System.exit(0);
		

	}

}
