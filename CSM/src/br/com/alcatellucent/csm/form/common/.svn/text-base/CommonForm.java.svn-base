package br.com.alcatellucent.csm.form.common;

import org.apache.struts.action.ActionForm;

/**
 * @author Fernando Caruso
 * 
 */
public class CommonForm extends ActionForm {

    private static final long serialVersionUID = -3947676770239713472L;

    private String action;
    private String refresh;

    /**
     * @return the action
     */
    public String getAction() {
	if (null == action) {
	    action = new String();
	}
	return action;
    }

    /**
     * @param action
     *                the action to set
     */
    public void setAction(String action) {
	this.action = action;
    }

    /**
     * @return the refresh
     */
    public String getRefresh() {
	return refresh;
    }

    /**
     * @param refresh
     *                the refresh to set
     */
    public void setRefresh(String refresh) {
	this.refresh = refresh;
    }

}
