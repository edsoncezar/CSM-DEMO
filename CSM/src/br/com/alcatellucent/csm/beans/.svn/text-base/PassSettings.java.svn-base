package br.com.alcatellucent.csm.beans;

import br.com.alcatellucent.csm.beans.common.CustomObject;

/**
 * 
 * @author Igor Ivanoff Takats
 * 
 */

public class PassSettings extends CustomObject {

	private Integer failedAttempts;
	private Integer minimunLength;
	private Integer levelSecurity;
	private Integer enforceHistoryQty;
	private Integer enforceChangingDays;
	private Integer lockDurationMin;
	private Boolean enforceHistoryEnable;
	private Boolean enforceChangingEnable;
	private Boolean lockDurationEnable;
	private Boolean flagFailedAttempts;
	
	public PassSettings(){
		this.enforceHistoryEnable=false;
		this.enforceChangingEnable=false;
		this.lockDurationEnable=false;
		this.flagFailedAttempts=false;
	}
	
	public Integer getFailedAttempts() {
		return failedAttempts;
	}

	public void setFailedAttempts(Integer failedAttempts) {
		this.failedAttempts = failedAttempts;
	}

	public Integer getMinimunLength() {
		return minimunLength;
	}

	public void setMinimunLength(Integer minimunLength) {
		this.minimunLength = minimunLength;
	}

	public Integer getLevelSecurity() {
		return levelSecurity;
	}

	public void setLevelSecurity(Integer levelSecurity) {
		this.levelSecurity = levelSecurity;
	}

	public Integer getEnforceHistoryQty() {
		return enforceHistoryQty;
	}

	public void setEnforceHistoryQty(Integer enforceHistoryQty) {
		this.enforceHistoryQty = enforceHistoryQty;
	}

	public Integer getEnforceChangingDays() {
		return enforceChangingDays;
	}

	public void setEnforceChangingDays(Integer enforceChangingDays) {
		this.enforceChangingDays = enforceChangingDays;
	}

	public Integer getLockDurationMin() {
		return lockDurationMin;
	}

	public void setLockDurationMin(Integer lockDurationMin) {
		this.lockDurationMin = lockDurationMin;
	}

	public Boolean getEnforceHistoryEnable() {
		return enforceHistoryEnable;
	}

	public void setEnforceHistoryEnable(Boolean enforceHistoryEnable) {
		this.enforceHistoryEnable = enforceHistoryEnable;
	}

	public Boolean getEnforceChangingEnable() {
		return enforceChangingEnable;
	}

	public void setEnforceChangingEnable(Boolean enforceChangingEnable) {
		this.enforceChangingEnable = enforceChangingEnable;
	}

	public Boolean getLockDurationEnable() {
		return lockDurationEnable;
	}

	public void setLockDurationEnable(Boolean lockDurationEnable) {
		this.lockDurationEnable = lockDurationEnable;
	}

	public Boolean getFlagFailedAttempts() {
		return flagFailedAttempts;
	}

	public void setFlagFailedAttempts(Boolean flagFailedAttempts) {
		this.flagFailedAttempts = flagFailedAttempts;
	}

}
